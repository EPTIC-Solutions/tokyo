{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "eptic/tokyo",
  "description": "Local php development for linux artisans.",
  "type": "library",
  "license": "GNU GPLv3",
  "require": {
    "mnapoli/silly": "^1.8",
    "php": ">=8.1",
    "ext-json": "*",
    "illuminate/collections": "^10.0",
    "symfony/var-dumper": "^6.3",
    "symfony/event-dispatcher": "^6.3",
    "symfony/process": "^6.3",
    "php-di/php-di": "^7.0",
    "psy/psysh": "^0.11.20"
  },
  "autoload": {
    "psr-4": {
      "Tokyo\\": "src/Tokyo"
    },
    "files": [
      "src/includes/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Cristian Bilu"
    }
  ],
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ]
  },
  "bin": [
    "tokyo"
  ],
  "require-dev": {
    "laravel/pint": "^1.10"
  }
}
